    <line lang1="Je m'baladais sur l'avenue," someNumber="None" image="f.jpg" lang2="Я прогуливался по проспекту,">
      <audioTime in="0" out="10526" />
    </line>
    <line lang1="Le cœur ouvert à l'inconnu," someNumber="None" image="f.jpg" lang2="С сердцем открытым незнакомцу,">
      <audioTime in="10526" out="12715" />
    </line>
    <line lang1="J'avais envie de dire" someNumber="None" image="f.jpg" lang2="У меня было желание сказать">
      <audioTime in="12715" out="14753" />
    </line>
    <line lang1="bonjour à n'importe qui." someNumber="None" image="f.jpg" lang2="здравствуй неважно кому.">
      <audioTime in="14753" out="16365" />
    </line>
    <line lang1="N'importe qui se fut toi," someNumber="None" image="f.jpg" lang2="Неизвестно кто стала ты,">
      <audioTime in="16365" out="19125" />
    </line>
    <line lang1="Je te dis n'importe quoi." someNumber="None" image="f.jpg" lang2="Я тебе говорил неважно что.">
      <audioTime in="19125" out="21188" />
    </line>
    <line lang1="Il suffisait de te parler" someNumber="None" image="f.jpg" lang2="Было достаточно с тобой поговорить">
      <audioTime in="21188" out="23370" />
    </line>
    <line lang1="Pour t'apprivoiser." someNumber="None" image="f.jpg" lang2="Чтобы приручить тебя.">
      <audioTime in="23370" out="24996" />
    </line>
    <line lang1="Oh! Champs-Elysées. Oh! Champs-Elysées." someNumber="None" image="f.jpg" lang2="О, Елисейские поля! О, Елисейские поля!">
      <audioTime in="24996" out="32680" />
    </line>
