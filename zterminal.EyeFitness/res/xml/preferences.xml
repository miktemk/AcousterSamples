<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="Basic Options">

        <CheckBoxPreference
                android:key="do_music"
                android:title="Music"
                android:defaultValue="true"
                android:summary="Play annoying music."
        />
        <CheckBoxPreference
                android:key="do_arrows"
                android:title="Arrows"
                android:defaultValue="true"
                android:summary="Show annoying arrows."
        />
        <org.acouster.android.ui.SeekBarPreference android:key="intensity"
	        android:title="Duration"
	        android:defaultValue="5"
	        android:text=" (factor of)"
	        android:max="10"
        />
        

    </PreferenceCategory>

    

</PreferenceScreen>